{
  "name": "release-page-reload",
  "version": "1.2.3",
  "description": "Show a popup or refresh your app when there is a new GitHub version released ⚡️",
  "keywords": ["version", "reload", "github", "releases", "release", "github release", "repo version"],
  "bugs": {
    "url": "https://github.com/releasepage/reload.js/issues",
    "email": "james@releasepage.co"
  },
  "repository": "releasepage/reload.js",
  "main": "./build/js/reload.js",
  "minified": "./build/js/reload.min.js",
  "browser": "./build/js/reload.amd.js",
  "module": "./build/js/reload.amd.js",
  "scripts": {
    "clean": "rm -r ./build/*",
    "test": "eslint ./reload.js",
    "build": "rollup -c && npm run build:min && npm run build:amd && npm run build:combined && npm run build:sass",
    "watch": "rollup -c -w && npm run build:min:watch && npm run build:amd:watch",
    "build:sass": "node-sass --output-style compressed themes/basic.scss ./build/css/reload-basic.css",
    "build:min": "rollup -c ./rollup.min.config.js",
    "build:amd": "rollup -c ./rollup.amd.config.js",
    "build:combined": "cat ./node_modules/release-page-version/build/js/version.min.js ./build/js/reload.min.js > ./build/js/reload.combined.js",
    "build:min:watch": "rollup -w -c ./rollup.min.config.js",
    "build:amd:watch": "rollup -w -c ./rollup.amd.config.js"
  },
  "author": "James Ivings <james@releasepage.co>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^6.10.3",
    "lodash.defaults": "^4.2.0",
    "node-sass": "^4.5.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "microevent": "^1.0.0",
    "semver": "^5.3.0",
    "release-page-version": "^1.1.2"
  },
  "dependencies": {}
}
